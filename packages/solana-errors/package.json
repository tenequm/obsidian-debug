{
  "name": "solana-idls",
  "version": "0.1.0",
  "description": "Comprehensive Solana IDL database with errors, instructions, and account metadata from 41+ protocols",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --clean",
    "prebuild": "pnpm generate",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
    "type-check": "tsc --noEmit",
    "generate": "tsx scripts/generate.ts",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "yaml": "^2.8.1",
    "zx": "^8.8.5"
  },
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "sideEffects": [
    "src/generated/protocols.ts"
  ],
  "license": "MIT",
  "keywords": [
    "solana",
    "idl",
    "errors",
    "instructions",
    "accounts",
    "anchor",
    "blockchain",
    "error-codes",
    "debugging",
    "web3"
  ],
  "files": [
    "dist/**",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tenequm/obsidian-debug",
    "directory": "packages/solana-errors"
  },
  "bugs": {
    "url": "https://github.com/tenequm/obsidian-debug/issues"
  },
  "homepage": "https://github.com/tenequm/obsidian-debug/tree/main/packages/solana-errors#readme"
}
