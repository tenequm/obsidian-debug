{
  "name": "obsidian-debug",
  "version": "0.1.0",
  "private": true,
  "description": "AI-powered Solana transaction debugger",
  "scripts": {
    "dev": "turbo dev --log-prefix=none",
    "build": "turbo build",
    "lint": "biome check",
    "format": "biome check --write",
    "check": "turbo type-check && biome check --write",
    "type-check": "turbo type-check",
    "clean": "rm -rf node_modules packages/*/node_modules packages/*/.next",
    "test": "turbo test"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.7",
    "husky": "^9.1.7",
    "turbo": "^2.5.8",
    "ultracite": "5.6.4"
  },
  "packageManager": "pnpm@10.19.0"
}
