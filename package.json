{
  "name": "obsidian-debug",
  "version": "0.1.0",
  "private": true,
  "description": "AI-powered Solana transaction debugger",
  "scripts": {
    "dev": "pnpm --filter @obsidian-debug/app dev",
    "build": "pnpm --filter @obsidian-debug/app build",
    "start": "pnpm --filter @obsidian-debug/app start",
    "lint": "pnpm --filter @obsidian-debug/app lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rm -rf node_modules packages/*/node_modules packages/app/.next"
  },
  "devDependencies": {
    "prettier": "^3.6.2"
  },
  "packageManager": "pnpm@10.17.1"
}
